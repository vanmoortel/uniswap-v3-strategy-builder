  "description": "Static website to analyse the profitability of your uniswap V3 strategy.(minimum viable product)",
  "main": "index.js",
  "scripts": {
    "heroku-prebuild": "npm install -g serve",
    "start": "serve -s build",
    "start:dev": "yarn run frontend:dev",
    "build": "yarn run frontend:build",
    "frontend:dev": "craco start",
    "frontend:build": "craco build",
    "build:remove": "del dist && del build",
    "log:remove": "del log",
    "test": "npm run test:eslint",
    "test:eslint": "eslint src/",
    "test:eslint-fix": "eslint src/ --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Nolan Vanmoortel",
  "license": "ISC",
